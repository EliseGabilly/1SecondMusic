<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fr.oneSecondmusic">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.1SecMusic">
        <activity
            android:name="com.fr.oneSecondmusic.activity.ResearchActivity"
            android:exported="true" />
        <activity
            android:name="com.fr.oneSecondmusic.activity.EditPlaylistActivity"
            android:exported="true" />
        <activity
            android:name="com.fr.oneSecondmusic.activity.MainActivity"
            android:exported="true" />
        <activity
            android:name="com.fr.oneSecondmusic.activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>